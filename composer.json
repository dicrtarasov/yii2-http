{
    "name": "dicr/yii2-http",
    "type": "yii2-extension",
    "description": "Утилиты HTTP для Yii2",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": ["dicr", "php", "yii2", "http"],
    "homepage": "https://gitlab.com/dicr/yii2-http",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-http.git"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-httpclient": "2.*",
        "dicr/yii2-helper": "3.*",
        "simplehtmldom/simplehtmldom": "~2.0-RC2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.*"
    },
    "repositories": [
        {"type": "gitlab", "url": "https://gitlab.com/dicr/yii2-helper.git"},
        {"type": "composer", "url": "https://asset-packagist.org"}
    ],
    "autoload": {
        "psr-4": {
            "dicr\\http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    }
}
