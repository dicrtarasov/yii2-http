{
    "name": "dicr/yii2-http",
    "type": "yii2-extension",
    "description": "Утилиты HTTP для Yii2",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": ["dicr", "php", "yii2", "http"],
    "homepage": "https://gitlab.com/dicr/yii2-http",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-http.git"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "yiisoft/yii2": "~2.0.32",
        "yiisoft/yii2-httpclient": "~2.0.12",
        "simplehtmldom/simplehtmldom": "~2.0-RC2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~7.5.20"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }, {
            "type": "git",
            "url": "https://git.code.sf.net/p/simplehtmldom/repository"
        }, {
            "type": "git",
            "url": "https://gitlab.com/dicr/yii2-settings.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "dicr\\http\\": "src/"
        }
    }
}
